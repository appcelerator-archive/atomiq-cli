  },
  "repository": "atomiqio/atomiq-cli",
  "dependencies": {
    "babel": "*",
    "babel-cli": "*",
    "babel-core": "*",
    "babel-eslint": "*",
    "babel-plugin-syntax-async-functions": "*",
    "babel-plugin-transform-regenerator": "*",
    "babel-polyfill": "*",
    "babel-preset-es2015": "*",
    "babel-runtime": "*",
    "chalk": "*",
    "commander": "*",
    "debug": "*",
    "eslint": "*",
    "eslint-plugin-babel": "^3.2.0",
    "inquirer": "^1.0.2",
    "node-fetch": "*",
    "nodemon": "*",
    "nunjucks": "*",
    "optimist": "*",
    "prompt": "*",
    "ramda": "*",
    "rimraf": "*",
    "semver": "*",
    "shelljs": "*",
    "source-map-support": "*",
    "watch": "^0.17.1"
  },
  "devDependencies": {
    "esformatter": "*",
    "isparta": "^4.0.0",
