{
  "name": "atomiq-cli",
  "version": "1.0.0-alpha.14",
  "description": "Atomiq CLI",
  "license": "MIT",
  "scripts": {
    "test": "babel-node ./node_modules/.bin/isparta cover --report html ./node_modules/mocha/bin/_mocha -- --recursive src/test",
